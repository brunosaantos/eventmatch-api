{
  "name": "eventmatch-api",
  "version": "1.0.0",
  "description": "Eventmatch API",
  "main": "index.js",
  "scripts": {
    "precommit": "npm run lint",
    "test": "npm run test-unit & npm run test-integration",
    "test-integration": "NODE_ENV=test mocha --opts tests/integration/mocha.opts tests/integration/*.js",
    "test-unit": "NODE_ENV=test mocha --opts tests/unit/mocha.opts tests/unit/**/*.js",
    "push:tags": "git push origin --tags && git push github --tags",
    "push:master": "git push origin master && git push github master",
    "push": "npm run push:master && npm run push:tags",
    "start:dev": "NODE_ENV=dev nodemon index.js localhost 3030 --exec babel-node",
    "start": "NODE_ENV=prod nodemon index.js localhost 3030 --exec babel-node",
    "lint": "./node_modules/eslint/bin/eslint.js ."
  },
  "repository": {
    "type": "git",
    "url": "http://187.7.106.14:7963/eventmatch/eventmatch-api.git"
  },
  "keywords": [
    "eventmatch",
    "api",
    "cordova",
    "ionic"
  ],
  "author": "Bruno Santos",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "geolib": "^2.0.21",
    "is_js": "^0.8.0",
    "jsonwebtoken": "^7.0.0",
    "lodash": "^4.11.2",
    "md5": "^2.1.0",
    "moment": "^2.14.1",
    "mysql": "^2.10.2",
    "nodemon": "^1.11.0",
    "restify": "^4.0.4",
    "sequelize": "^3.22.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^3.11.1",
    "husky": "^0.11.5",
    "istanbul": "^0.4.5",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.1",
    "supertest": "^2.0.0",
    "testdouble": "^1.6.1"
  }
}
